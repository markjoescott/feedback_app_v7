<div class="page-header">
  <h1>Thanks, your questionnaire is complete!</h1>
</div>

<p><a href="/questionnaires/new" class="btn btn-primary">Add a New Questionnaire</a></p>
<p><a href="/create_answer" class="btn btn-danger">See current questionnaires</a></p>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Course</th>
      <th>User</th>
      <th></th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>


  <tbody>

    <% @questionnaires.each do |questionnaire| %>
      <tr>
        <td><%= questionnaire.questionnaire_name %></td>
        <td><%= questionnaire.course_id %></td>
        <td><%= questionnaire.user_id %></td>

        <td>


    <%if questionnaire.user_id == current_user.id %>

          <a href="/questionnaires/<%= questionnaire.id %>" class="btn btn-primary">
            <i class="fa fa-search-plus"></i>
          </a>
        </td>
        <td>
          <a href="/questionnaires/<%= questionnaire.id %>/edit" class="btn btn-warning">
            <i class="fa fa-edit"></i>
          </a>
        </td>
        <td>
          <a href="/delete_questionnaire/<%= questionnaire.id %>" class="btn btn-danger">
            <i class="fa fa-trash-o"></i>
          </a>
          <% end %>
        </td>
      </tr>

    <% end %>
  </tbody>
</table>

